{
  "nixEnvSelector.nixFile": "${workspaceRoot}/shell.nix",
  "nix.serverPath": "nil",
  "nix.formatterPath": ["nix", "fmt", "--", "-"],
  "alejandra.program": "alejandra",
  "nix.enableLanguageServer": true,
  "files.associations": {
    "flake.lock": "json"
  },
  "files.exclude": {
    "mutable/": true,
    ".direnv": true,
    ".devenv": true
  },
  "search.exclude": {
    "mutable/": true,
    ".direnv": true,
    ".devenv": true
  }
}
